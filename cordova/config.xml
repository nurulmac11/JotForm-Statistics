<?xml version='1.0' encoding='utf-8'?>
<widget id="com.jotform.nrlmcn" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>JotForm Statistics</name>
    <description>
        View and analyse your jotform form submissions.
    </description>
    <author email="nurullah201@gmail.com" href="http://nmacun.com">
        nurulmac11
    </author>
    <icon density="mdpi" height="57" platform="android" src="res/icon/android/jot.png" width="57" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <splash density="land-hdpi" src="res/screen/android/screen-hdpi-landscape.png" />
        <splash density="port-hdpi" src="res/screen/android/screen-hdpi-portrait.png" />
        <preference name="SplashMaintainAspectRatio" value="true" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-splashscreen" spec="^4.0.3" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
    <plugin name="cordova-plugin-x-socialsharing" spec="^5.2.0" />
    <engine name="android" spec="^6.2.3" />
    <engine name="browser" spec="^5.0.3" />
</widget>
